public class Problem_1 {

	public static void main(String[] args) {
		percentEven();
	}
	
	public static void percentEven() {
		
		int[] arr = {6,2,9,11,3};
		double nums = 0;
		double count = 0;
		for (int index = 0; index<arr.length; index++) {
			if (arr[index] % 2 == 0) {
				count++;
				nums = count*100/arr.length;			
			}
		}
		System.out.print(nums);
	}
}

// Output:  40.0

